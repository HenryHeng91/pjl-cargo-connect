<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="pjl-cargo-connect">
        <addNotNullConstraint columnDataType="INT" columnName="COUNTRY_ID" tableName="MASTERDATA_CARGO_PORTS"
                              validate="true"/>
    </changeSet>
    <changeSet id="2" author="pjl-cargo-connect">
        <addNotNullConstraint columnDataType="VARCHAR(10)" columnName="PORT_CODE" tableName="MASTERDATA_CARGO_PORTS"
                              validate="true"/>
    </changeSet>
    <changeSet id="3" author="pjl-cargo-connect">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="PORT_NAME" tableName="MASTERDATA_CARGO_PORTS"
                              validate="true"/>
    </changeSet>
    <changeSet id="4" author="pjl-cargo-connect">
        <addNotNullConstraint columnDataType="INT" columnName="PORT_TYPE" tableName="MASTERDATA_CARGO_PORTS"
                              validate="true"/>
    </changeSet>
</databaseChangeLog>